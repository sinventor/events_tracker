<% if user_signed_in? %>
  <li><%= link_to 'Profile', edit_user_registration_path, class: 'list-nav-link' %></li>
  <li><%= link_to 'Sign out', destroy_user_session_path, method: :delete, class: 'list-nav-link' %></li>
<% else %>
  <li><%= link_to 'Sign in', user_session_path, class: 'list-nav-link' %></li>
  <li><%= link_to 'Register', new_user_registration_path, class: 'list-nav-link' %></li>
<% end %>